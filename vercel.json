{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "HEAD", "OPTIONS"]
    }
  ],
  "functions": {
    "src/validation/*.ts": {
      "runtime": "nodejs12.x"
    },
    "src/utils/response.list.ts": {
      "runtime": "nodejs12.x"
    }
  
  }
}